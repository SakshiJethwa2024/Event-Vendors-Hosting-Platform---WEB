            <%})%> 
         <% } %> -->
        </div>
    </div>


    <div class="modal" id="filterModal" tabindex="-1" aria-labelledby="filterLabel" aria-hidden="true">
        <div class="modal-dialog form modal-xl">
            <div class="modal-content form-content">
                <div class="modal-header">
                    <h5 class="modal-title form-title" id="filterModalLabel"><i class="fa fa-sliders"></i> Filter</h5>
                    <button type="button" class="btn-close form-close" data-bs-dismiss="modal"
                        aria-label="Close"></button>
                </div>
                <div class="modal-body row">
                    <div class="filter-topics col-md-6">
                        <h5>Number of Days</h5>
                        <div class="options">
                            <div class="row filter-rows">
                                <div class="col-md-3 col-sm-2 col-2"><input type="radio" name="days"
                                        id="less-than-100"></div>
                                <div class="col-6">&lt 1 day</div>
                            </div>
                            <div class="row filter-rows">
                                <div class="col-md-3 col-sm-2 col-2"><input type="radio" name="days"
                                        id="hundred-twofifty"></div>
                                <div class="col-6">1 day</div>
                            </div>
                            <div class="row filter-rows">
                                <div class="col-md-3 col-sm-2 col-2"><input type="radio" name="days"
                                        id="twofifty-fivehundred">
                                </div>
                                <div class="col-6">2 days</div>
                            </div>
                            <div class="row filter-rows">
                                <div class="col-md-3 col-sm-2 col-2"><input type="radio" name="days"
                                        id="fivehundred-thousand">
                                </div>
                                <div class="col-6">3 days</div>
                            </div>
                            <div class="row filter-rows">
                                <div class="col-md-3 col-sm-2 col-2"><input type="radio" name="days"
                                        id="fivehundred-thousand">
                                </div>
                                <div class="col-6">&gt 3 days</div>
                            </div>
                        </div>
                    </div>
                  
                    <div class="filter-topics col-md-6">
                        <h5>Services</h5>
                        <div class="options">
                            <div class="row filter-rows">
                                <div class="col-md-2 col-sm-2 col-2"><input type="radio" name="services"
                                        id="less-than-1000"></div>
                                <div class="col-10">Photo Package</div>
                            </div>
                            <div class="row filter-rows">
                                <div class="col-md-2 col-sm-2 col-2"><input type="radio" name="services"
                                        id="one-five-thousand"></div>
                                <div class="col-10"> Photo + Video</div>
                            </div>
                            <div class="row filter-rows">
                                <div class="col-md-2 col-sm-2 col-2"><input type="radio" name="services"
                                        id="five-two-thousand">
                                </div>
                                <div class="col-10">Pre-Wedding Package</div>
                            </div>
                            <div class="row filter-rows">
                                <div class="col-md-2 col-sm-2 col-2"><input type="radio" name="services"
                                        id="two-three-thousand">
                                </div>
                                <div class="col-10">Studio Photography</div>
                            </div>
                            <div class="row filter-rows">
                                <div class="col-md-2 col-sm-2 col-2"><input type="radio" name="services"
                                        id="more-than-three-thousand">
                                </div>
                                <div class="col-10">Cinematography</div>
                            </div>
                        </div>
                    </div>  <hr>
                    <div class="filter-topics col-md-6">
                        <h5>Budget</h5>
                        <div class="options">
                            <div class="row filter-rows">
                                <div class="col-2"><input type="radio" name="budget" id="less-than-25000"></div>
                                <div class="col-7">&lt <i class="fa fa-inr"></i> 25000</div>
                            </div>
                            <div class="row filter-rows">
                                <div class="col-2"><input type="radio" name="budget" id="twofive-fifty-thousand"></div>
                                <div class="col-7"> <i class="fa fa-inr"></i>25000-<i class="fa fa-inr"></i>50000</div>
                            </div>
                            <div class="row filter-rows">
                                <div class="col-2"><input type="radio" name="budget" id="fifty-one-lakh">
                                </div>
                                <div class="col-7"> <i class="fa fa-inr"></i>50000-<i class="fa fa-inr"></i>100000
                                </div>
                            </div>
                            <div class="row filter-rows">
                                <div class="col-2"><input type="radio" name="budget" id="one-two-lakh">
                                </div>
                                <div class="col-7"> <i class="fa fa-inr"></i>100000-<i class="fa fa-inr"></i>200000
                                </div>
                            </div>
                            <div class="row filter-rows">
                                <div class="col-2"><input type="radio" name="budget" id="two-five-lakh">
                                </div>
                                <div class="col-7"> <i class="fa fa-inr"></i>200000-<i class="fa fa-inr"></i>500000
                                </div>
                            </div>
                            <div class="row filter-rows">
                                <div class="col-2"><input type="radio" name="budget" id="more-than-five-lakh">
                                </div>
                                <div class="col-7">&gt <i class="fa fa-inr"></i>500000</div>
                            </div>
                        </div>
                    </div>
                   
                    <div class="filter-topics col-md-6">
                        <h5>Rating</h5>
                        <div class="options">
                            <div class="row filter-rows">
                                <div class="col-md-3 col-sm-2 col-2"><input type="radio" name="rating"
                                        id="less-than-1000"></div>
                                <div class="col-5">Rated &lt 4</div>
                            </div>
                            <div class="row filter-rows">
                                <div class="col-md-3 col-sm-2 col-2"><input type="radio" name="rating"
                                        id="one-five-thousand"></div>
                                <div class="col-5"> Rated 4+</div>
                            </div>
                            <div class="row filter-rows">
                                <div class="col-md-3 col-sm-2 col-2"><input type="radio" name="rating"
                                        id="five-two-thousand">
                                </div>
                                <div class="col-5">Rated 4.5+</div>
                            </div>
                            <div class="row filter-rows">
                                <div class="col-md-3 col-sm-2 col-2"><input type="radio" name="rating"
                                        id="two-three-thousand">
                                </div>
                                <div class="col-5">Rated 4.8+</div>
                            </div>

                        </div>
                    </div>  <hr>

                </div>
                <div class="modal-footer" style="display: flex; flex-direction: column;">
                    <button class="filter-result-btn" id="filter-result">View Result</button>
                </div>
            </div>
        </div>
    </div>

    <script>


        let menu = document.querySelector("#menu-btn"),
            navbar = document.querySelector(".navbar");
        var main = document.querySelector(".main");

        function navBarToggle() {
            menu.classList.toggle('fa-window-close');
            navbar.classList.toggle('navbar-active');
            navbar.classList.toggle('navigation-bar');
        }


        function favourite() {
            var like = document.querySelector(".favourites");
            if (like.style.color == 'red') {
                like.style.color = 'grey'
            }
            else {
                like.style.color = 'red'
            }
        }

        $(document).ready(function () {
            $('[data-bs-toggle="tooltip"]').tooltip();
        })
    </script>
</body>

</html>