<div class="modal" id="signupModal" tabindex="-1" aria-labelledby="signupLabel" aria-hidden="true">
    <div class="modal-dialog form">
        <div class="modal-content form-content">
            <div class="modal-header">
                <h5 class="modal-title form-title" id="exampleModalLabel">Sign Up</h5>
                <button type="button" class="btn-close form-close" data-bs-dismiss="modal"
                    aria-label="Close"></button>
            </div>
            <div class="modal-body" style="display: flex; flex-direction: column;">
                <form action="/signup" method="POST" onsubmit="Sign(event)">

                    <div class="field input-field">
                        <input type="text" id="username" placeholder="Username" class="form-input" name="username">
                        <p class="username_warning" style="display:block; color: white;"></p>
                        <i class="fa fa-solid fa-user user"></i>
                    </div>

                    <div class="field input-field">
                        <input type="email" id="email" placeholder="Email" class="form-input" name="email">
                        <span class="email_warning" style="display: none; color: white;"></span>
                        <i class="fa fa-envelope email"></i>
                    </div>

                    <div class="field input-field">
                        <select id="role" name="role" class="form-input">
                            <option value="" disabled selected>Role</option>
                            <option value="user">User</option>
                            <option value="vendor">Vendor</option>
                        </select>
                        <p class="role_warning" style=" display:block;color: white;"></p>
                        <i class="fa fa-solid fa-plus role"></i>
                    </div>

                    <div class="field input-field">
                        <input type="password" id="password" placeholder="Create password" class="form-password" name="password">
                        <p class="password_condition" style="font-size: 10px; color: white;"></p>
                        <i class="fa fa-lock pw_lock"></i>
                        <i class="fa fa-eye-slash pw_hide"></i>
                    </div>

                    <div class="field input-field">
                        <input type="password" id="confirm_password" placeholder="Confirm password"
                            class="form-password">
                        <span class="confirm_pwd_warn"
                            style="display: none; color: white; font-weight: 500;"></span>
                        <i class="fa fa-lock pw_lock"></i>
                        <i class="fa fa-eye-slash pw_hide"></i>
                    </div>

                    <div class="field button-field">
                        <button id="signup1">Signup</button>
                    </div>
                </form>
                <div class="form-link">
                    <span>Already have an account? <a href="#loginModal" class="link login-link" id="login-link-su">Login</a></span>
                </div>

